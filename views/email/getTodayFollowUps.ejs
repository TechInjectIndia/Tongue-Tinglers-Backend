<div class="container">
    <h1>Follow-Up Reminder</h1>
    <p>Hello <%= userName %>,</p>
    <p>You have the following follow-ups scheduled for today:</p>
    <ul>
        <% followUps.forEach(function(followUp) { %>
            <li><strong>Lead:</strong> <%= followUp.leadName %> - <strong>Date:</strong> <%= followUp.followUpDate %></li>
        <% }); %>
    </ul>
    <div class="footer">
        <p>Thank you!</p>
    </div>
</div>
